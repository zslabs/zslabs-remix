{
  "private": true,
  "name": "zslabs-remix",
  "description": "Portfolio site for Zach Schnackel; built with Remix",
  "license": "MIT",
  "browserslist": [
    ">5%",
    "not IE 11",
    "not dead",
    "not op_mini all"
  ],
  "scripts": {
    "build": "npm run build:css && remix build",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "postcss styles --base styles --dir app/styles -w",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "build:icons": "tsx scripts/icons.ts",
    "type:check": "tsc --project tsconfig.json --pretty --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@netlify/classnames-template-literals": "1.0.3",
    "@remix-run/node": "1.6.0",
    "@remix-run/react": "1.6.0",
    "@remix-run/serve": "1.6.0",
    "@remix-run/vercel": "1.6.0",
    "@types/fs-extra": "9.0.13",
    "cssnano": "5.1.12",
    "esbuild": "0.14.46",
    "eslint-plugin-react-perf": "3.3.1",
    "framer-motion": "6.3.11",
    "mdx-bundler": "9.0.1",
    "nanospinner": "1.1.0",
    "prettier": "2.7.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tsx": "3.4.3"
  },
  "devDependencies": {
    "@iconify/tools": "2.0.18",
    "@remix-run/dev": "1.6.0",
    "@tailwindcss/line-clamp": "0.4.0",
    "@types/node": "18.0.0",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "autoprefixer": "10.4.7",
    "concurrently": "7.2.2",
    "eslint": "8.18.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.2",
    "postcss": "8.4.14",
    "postcss-cli": "9.1.0",
    "postcss-import": "14.1.0",
    "tailwindcss": "3.1.3",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": "14.x"
  },
  "sideEffects": false
}
